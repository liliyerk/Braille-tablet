from tkinter import *
def onclick1():
    import winsound
    winsound.Beep(800, 300)
def onclick2():
    import winsound
    winsound.Beep(600, 300)
root = Tk()

topFrame = Frame(root)
topFrame.pack()
bottomFrame = Frame(root)
bottomFrame.pack(side=BOTTOM)

button1 = Button(topFrame, text="Next Page", fg="red", command=onclick1)
button2 = Button(bottomFrame, text="Previous Page", fg="green", command=onclick2)

button1.pack()
button2.pack()

root.mainloop()
